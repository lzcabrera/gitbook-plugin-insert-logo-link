{
  "name": "gitbook-plugin-insert-logo-link",
  "version": "1.0.0",
  "description": "Gitbook plugin to inserts a logo wrapped in a link",
  "main": "index.js",
  "author": {
    "email": "lzcabrera@gmail.com",
    "name": "lzcabrera"
  },
  "bugs": {
    "url": "https://github.com/lzcabrera/gitbook-plugin-insert-logo-link/issues"
  },
  "engines": {
    "gitbook": ">1.x.x"
  },
  "gitbook": {
    "properties": {
      "src": {
        "default": "http://examples/logo.png",
        "title": "Image",
        "type": "string"
      },
      "url": {
        "default": "http://examples",
        "title": "Logo URL",
        "type": "string"
      }
    }
  },
  "keywords": [
    "gitbook",
    "plugin",
    "insert-logo-link",
    "github"
  ],
  "maintainers": [
    {
      "email": "lzcabrera@gmail.com",
      "name": "lzcabrera"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzcabrera/gitbook-plugin-insert-logo-link.git"
  }
}
